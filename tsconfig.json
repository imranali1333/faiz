{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*.ts","src/**/*" , "src/types/**/*" ],
  "exclude": ["node_modules"]
}